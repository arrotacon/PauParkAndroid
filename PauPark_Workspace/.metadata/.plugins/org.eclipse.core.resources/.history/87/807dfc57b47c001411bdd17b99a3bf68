package fr.univpau.pauparkdroid;

import android.content.Context;
import android.content.Intent;
import android.util.AttributeSet;
import android.view.View;
import android.widget.AdapterView;
import android.widget.AdapterView.OnItemSelectedListener;

public class OnHomeItemSelectedListener implements OnItemSelectedListener {
	
	public OnHomeItemSelectedListener(Context context, AttributeSet attrs, int defStyleAttr) {
		super();
		// TODO Auto-generated constructor stub
	}

	public void onItemSelected(AdapterView<?> parent, View view, int position,
			   long id) {
		
		Intent i = new Intent();
		i.setClass(Home.this, Parking.class);
		
			 }

			  @Override
			 public void onNothingSelected(AdapterView<?> arg0) {
			  // TODO Auto-generated method stub
			  }

}
